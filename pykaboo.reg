[HKEY_CLASSES_ROOT\SystemFileAssociations\.png\shell\Hide]
@="Hide files in this image with Pykaboo"

[HKEY_CLASSES_ROOT\SystemFileAssociations\.png\shell\Hide\command]
@="\"C:\\Windows\\System32\\cmd.exe\" /c python \"C:\\scripts\\steganodrive\\pykaboo_ui.py\" hide \"%1\""

[HKEY_CLASSES_ROOT\SystemFileAssociations\.png\shell\Unhide]
